  "Debug": false,
  "IgnoreDefaults": false,
  "SpacesAfterTabs": false,
  "NoColor": false,
  "Exclude": [
    "\\.git",
    "node_modules",
    "coverage\\.out",
    "coverage\\.html",
    "\\.DS_Store",
